package sec01.exam01;

public class DmbCellPhone extends CellPhone{
	int cannel;
	
	DmbCellPhone(String mode, String color , int channel){
		this.model = model;
		this.color = color;
		this.channel = channel;
	}

	void turnOnDumb() {
		System.out.println("채널"+channel1+"번 DMB방송 수신을 시작합니다");
		
	}
	void changeChannelDme(int channel) {
		this.channel1 = channel1;
		System.out.println("채널"+channel+"번으로 바꿉니다");
	}
	void turnOffDmb() {
		System.out.println("DMB 방송 수신을 멈춥니다");
	}
}
